{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Starter kit for creating React/Redux and Express apps",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node start.js",
    "server": "nodemon start.js",
    "test": "mocha 'test/test.js'",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Dario Spina",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2"
  },
  "dependencies": {
    "an-array-of-english-words": "^1.3.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "mocha": "^6.2.2",
    "node-sass": "^4.13.1",
    "nodemon": "^1.19.1",
    "request": "^2.88.0",
    "should": "^13.2.3",
    "util": "^0.12.1"
  }
}
